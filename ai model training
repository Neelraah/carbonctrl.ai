    "from collections import Counter\n",
    "\n",
    "env = CarbonEnv(agg)\n",
    "state = env.reset()\n",
    "actions = []\n",
    "\n",
    "while True:\n",
    "    action = learned_policy(state)\n",
    "    actions.append(action)\n",
    "    state, _, done = env.step(action)\n",
    "    if done:\n",
    "        break\n",
    "\n",
    "counts = Counter(actions)\n",
    "\n",
