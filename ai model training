   "source": [
    "df = pd.read_csv(\"/kaggle/input/powergridsense-dataset/power_system_multiclass_anomaly_data.csv\")\n",
    "df[\"Timestamp\"] = pd.to_datetime(df[\"Timestamp\"])\n",
    "\n",
    "\n",
    "df = df.set_index(\"Timestamp\")\n",
    "agg = (\n",
    "    df\n",
    "    .groupby(\"Location\")\n",
    "    .resample(\"15min\")\n",
    "    .agg({\n",
    "        \"Power (kW)\": \"sum\"\n",
    "    })\n",
    "    .reset_index()\n",
    ")\n",
    "\n",
    "#making the power in kW-mins\n",
    "\n",
    "agg[\"E_actual\"] = agg[\"Power (kW)\"] / 60\n",
    "#converting to kWh\n",
    "agg = agg.sort_values(\"Timestamp\")\n",
    "room_mean = agg.groupby(\"Location\")[\"E_actual\"].transform(\"mean\")\n",
    "\n",
    "\n",
    "agg[\"hour\"] = agg[\"Timestamp\"].dt.hour\n",
    "agg[\"is_workday\"] = agg[\"Timestamp\"].dt.weekday < 5\n",
    "\n",
    "\n",
    "agg = agg.sort_values(\"Timestamp\")\n",
    "\n",
    "fine_baseline = (\n",
    "    agg\n",
    "    .groupby([\"Location\", \"hour\", \"is_workday\"])[\"E_actual\"]\n",
    "    .transform(lambda x: x.rolling(window=7, min_periods=3).mean())\n",
    ")\n",
    "\n",
    "agg[\"E_expected\"] = fine_baseline.fillna(room_mean)\n",
    "\n",
    "\n",
    "#>1 is the % that was inefficient \n",
    "agg[\"efficiency_index\"] = agg[\"E_actual\"] / agg[\"E_expected\"]\n",
    "\n",
    "#adding conceptual data columns \n",
    "agg[\"emission_factor\"] = agg[\"hour\"].apply(\n",
    "    lambda h: 0.82 if 9 <= h <= 18 else 0.62\n",
    ")\n",
    "\n",
    "agg[\"E_excess\"] = (agg[\"E_actual\"] - agg[\"E_expected\"]).clip(lower=0)\n",
    "\n",
    "\n",
    "agg[\"CO2e_avoidable\"] = agg[\"E_excess\"] * agg[\"emission_factor\"]\n",
    "\n",
    "agg[[\"E_excess\", \"emission_factor\", \"CO2e_avoidable\"]].describe()\n",
    "\n",
    "agg[\"reward_carbon\"] = -agg[\"CO2e_avoidable\"]\n",
    "\n",
    "agg[\"comfort_flag\"] = (\n",
    "    (agg[\"hour\"].between(9, 18)) &\n",
    "    (agg[\"E_actual\"] < 0.5 * agg[\"E_expected\"])\n",
    ").astype(int)\n",
    "\n",
    "agg[\"critical_load_flag\"] = 0\n",
    "\n",
    "STATE_COLUMNS = [\n",
    "    \"hour\",\n",
    "    \"is_workday\",\n",
    "    \"E_actual_t\",\n",
    "    \"E_actual_t-1\",\n",
    "    \"E_actual_t-2\",\n",
    "    \"E_expected\",\n",
    "    \"E_excess\",\n",
    "    \"CO2e_avoidable\"\n",
    "]\n",
    "\n",
    "\n",
    "def get_state(row):\n",
    "    return row[STATE_COLUMNS].values.astype(np.float32)\n",
    "\n",
    "\n",
    "agg[\"E_actual_t\"] = agg[\"E_actual\"]\n",
    "agg[\"E_actual_t-1\"] = agg[\"E_actual\"].shift(1)\n",
    "agg[\"E_actual_t-2\"] = agg[\"E_actual\"].shift(2)\n",
    "\n",
    "#agg.tail()\n",
    "\n",
    "required_cols = [\n",
    "    \"reward\",\n",
    "    \"E_actual\",\n",
    "    \"E_expected\",\n",
    "    \"E_excess\",\n",
    "    \"CO2e_avoidable\",\n",
    "    \"emission_factor\",\n",
    "    \"hour\",\n",
    "    \"is_workday\"\n",
    "]\n",
    "\n",
    "agg[[\"E_actual_t-1\", \"E_actual_t-2\"]] = (\n",
    "    agg[[\"E_actual_t-1\", \"E_actual_t-2\"]]\n",
    "    .fillna(method=\"bfill\")\n",
    ")\n",
    "\n",
    "\n",
    "missing = [c for c in required_cols if c not in agg.columns]\n",
    "missing\n",
    "\n",
    "agg.head()\n"
   ]
  },
  {
