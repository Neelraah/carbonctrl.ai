   "outputs": [],
   "source": [
    "class CarbonEnv:\n",
    "    def __init__(self, df, reduction_factor=0.2):\n",
    "        self.df = df.reset_index(drop=True)\n",
    "        self.reduction_factor = reduction_factor\n",
    "        self.current_step = 0\n",
    "        self.n_steps = len(df)\n",
    "\n",
    "    def reset(self):\n",
    "        self.current_step = 0\n",
    "        return get_state(self.df.iloc[self.current_step])\n",
    "\n",
    "    def step(self, action):\n",
    "        self.current_step += 1\n",
    "        done = self.current_step >= self.n_steps - 1\n",
    "    \n",
    "        row = self.df.iloc[self.current_step].copy()\n",
    "    \n",
    "        # base energy\n",
    "        E_actual_next = row[\"E_actual\"]\n",
    "    \n",
    "        # apply action\n",
    "        if action == ACTION_REDUCE:\n",
    "            reduction = self.reduction_factor * row[\"E_excess\"]\n",
    "            E_actual_next = max(E_actual_next - reduction, row[\"E_expected\"])\n",
    "    \n",
    "        # update row so get_state reflects action outcome\n",
    "        row[\"E_actual\"] = E_actual_next\n",
    "        row[\"E_excess\"] = max(E_actual_next - row[\"E_expected\"], 0)\n",
    "        row[\"CO2e_avoidable\"] = row[\"E_excess\"] * row[\"emission_factor\"]\n",
    "    \n",
    "        reward = -row[\"CO2e_avoidable\"]\n",
    "    \n",
    "        next_state = get_state(row)\n",
    "        return next_state, reward, done\n",
    "\n",
    "\n",
    "\n",
    "ACTION_NO_OP = 0          # do nothing\n",
    "ACTION_REDUCE = 1         # try to reduce consumption\n",
    "\n",
    "N_ACTIONS = 2"
   ]
  },
  {
