    "#transition test\n",
    "env = CarbonEnv(agg)\n",
    "rewards =[]\n",
    "\n",
    "for i in range(500):\n",
    "    action = np.random.choice([ACTION_NO_OP, ACTION_REDUCE])\n",
    "    \n",
    "    next_state, reward, done = env.step(action)\n",
    "    #print(f\"Step {i} | Action {action} | Reward {reward} | Done {done}\")\n",
    "    rewards.append(reward)\n",
    "    if done:\n",
    "        break\n",
    "\n",
    "rewards = np.array(rewards)\n",
    "print(np.sum(rewards != 0))"
   ]
