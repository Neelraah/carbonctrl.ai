   "source": [
    "def run_episode_with_policy(env, policy_fn):\n",
    "    state = env.reset()\n",
    "    total_reward = 0\n",
    "    while True:\n",
    "        action = policy_fn(state)\n",
    "        state, reward, done = env.step(action)\n",
    "        total_reward += reward\n",
    "        if done:\n",
    "            break\n",
    "    return total_reward\n",
    "\n",
    "def no_op_policy(state):\n",
    "    return ACTION_NO_OP\n",
    "\n",
    "def learned_policy(state):\n",
    "    with torch.no_grad():\n",
    "        s = torch.tensor(state, dtype=torch.float32).unsqueeze(0).to(device)\n",
    "        return policy_net(s).argmax().item()\n",
    "\n",
    "env1 = CarbonEnv(agg)\n",
    "env2 = CarbonEnv(agg)\n",
    "\n",
    "baseline_reward = run_episode_with_policy(env1, no_op_policy)\n",
    "learned_reward = run_episode_with_policy(env2, learned_policy)\n",
    "\n",
    "plt.figure(figsize=(6,4))\n",
    "plt.bar([\"Baseline\", \"Learned\"], [baseline_reward, learned_reward])\n",
    "plt.ylabel(\"Total Reward\")\n",
    "plt.title(\"Baseline vs Learned Policy\")\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "print(\"Baseline reward:\", baseline_reward)\n",
    "print(\"Learned reward:\", learned_reward)\n"
   ]
  },
