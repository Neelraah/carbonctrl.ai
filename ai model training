    "class QNetwork(nn.Module):\n",
    "    def __init__(self, state_size, action_size):\n",
    "        super().__init__()\n",
    "        self.net = nn.Sequential(\n",
    "            nn.Linear(state_size, 64),\n",
    "            nn.ReLU(),\n",
    "            nn.Linear(64, 64),\n",
    "            nn.ReLU(),\n",
    "            nn.Linear(64, action_size)\n",
    "        )\n",
    "\n",
    "    def forward(self, x):\n",
    "        return self.net(x)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "2b0e012a",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2026-02-04T16:59:12.324314Z",
     "iopub.status.busy": "2026-02-04T16:59:12.323717Z",
     "iopub.status.idle": "2026-02-04T16:59:12.328882Z",
